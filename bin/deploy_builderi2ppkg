#!/bin/sh

# READY

. /usr/local/bin/loadmyvars.sh

. /usr/local/bin/_deploy_include.sh

psnap

# unattended build
export BATCH=yes


if [ "x" == "x${I2PPKG}" ]; then
    echo "ERROR: I2PPKG not defined."
    exit 1
fi
if [ -e ${I2PPKG} ]; then
    rm ${I2PPKG}
fi


PORTNAME="security/i2p"
pkg install -y \
    math/gmp \
    java/openjdk7 \
    devel/apache-ant \
    devel/gettext-tools \
    archivers/unzip \
    devel/gettext-runtime
    #converters/libiconv
build_it_now ${PORTNAME}
