#!/bin/sh

# READY

######################################################################
# Script version is yymmdd-HHMMSS in UTC, date -u +%y%m%d-%H%M%S
######################################################################
SCRIPTVERSION=150618-192004

GOOD="
proxy
couch
owncloud
dtfc
ejabberd
murmur
vnc
gitlab
"

BAD="
baikal
couchdtfc
horde
"

DEV="
echoplexus
gitannex
gogs
phabricator
pydio
"

JAILNAMES="${GOOD} ${BAD} ${DEV}"

eza stop

for i in ${JAILNAMES} ; do
    if zfs list tank/ezjail/${i}@10.1-RELEASE-p12 2>/dev/null ; then
        zfs rollback tank/ezjail/${i}@10.1-RELEASE-p12
    fi
done
