#!/bin/sh

# READY

######################################################################
# Script version is yymmdd-HHMMSS in UTC, date -u +%y%m%d-%H%M%S
######################################################################
SCRIPTVERSION=150620-221104

GOOD="
squid
nginx
couch
dtfc
owncloud
ejabberd
murmur
vnc
gitlab
"

BAD="
baikal
couchdtfc
horde
"

DEV="
echoplexus
gitannex
gogs
phabricator
pydio
"

JAILNAMES="${GOOD} ${BAD} ${DEV}"

eza stop

for i in ${JAILNAMES} ; do
    if [ "xsquid" != "x${i}" ]; then
        if zfs list -H tank/ezjail/${i}@10.1-RELEASE-p12 2>/dev/null ; then
            zfs rollback tank/ezjail/${i}@10.1-RELEASE-p12
        fi
    fi
done
